   ],
   "source": [
    "n= int(input())\n",
    "w=list(map(int,input().split()))\n",
    "s=list(map(int,input().split()))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "32.0\n"
     ]
    }
   ],
   "source": [
    "sum_wights= sum([a*b for a,b in zip(w,s)])\n",
    "result= round((sum_wights / sum(s)),1)\n",
    "print(result)"
   ]
